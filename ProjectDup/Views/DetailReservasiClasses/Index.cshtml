        <td>
            @Html.DisplayFor(modelItem => item.tanggal_check_in)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tanggal_check_out)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.id_reservasi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.id_kamar)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id_detail_reservasi }) |
