@model IEnumerable<ProjectDup.Models.DetailReservasiClass>

@{
    ViewBag.Title = "Index";
}

<h2>Detail Reservasi</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.tanggal_check_in)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tanggal_check_out)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.id_reservasi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.id_kamar)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
